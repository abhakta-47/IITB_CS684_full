(** Simulation of Obc machines *)

open Names

type value =
  | Vint of int
  | Vfloat of float
  | Vbool of bool
  | Vstring of string
  | Vconstructor of constructor_name
  | Varray of value array
  | Vtuple of value list
  | Vrecord of (field_name * value) list

type env_value =
  | Evalue of value
  | Eenv of env

and env =
  env_value Idents.Env.t

let eval_ext_value env ev  
